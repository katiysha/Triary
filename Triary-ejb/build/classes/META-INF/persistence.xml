<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
  <persistence-unit name="Triary-ejbPU" transaction-type="JTA">
    <jta-data-source>jdbc/triary</jta-data-source>
    <class>model.Comment</class>
    <class>model.Diary</class>
    <class>model.Profile</class>
    <class>model.Publication</class>
    <class>model.Statistics</class>
    <class>model.Users</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <validation-mode>NONE</validation-mode>
    <properties>
    </properties>
  </persistence-unit>
</persistence>
